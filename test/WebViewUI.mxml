<?xml version="1.0" encoding="utf-8"?>
<s:Application xmlns:fx="http://ns.adobe.com/mxml/2009"
			   xmlns:s="library://ns.adobe.com/flex/spark"
			   xmlns:mx="library://ns.adobe.com/flex/mx"
			   minWidth="955"
			   minHeight="600"
			   xmlns:views="org.tinytlf.views.*">
	<s:layout>
		<s:BasicLayout/>
	</s:layout>
	
	<fx:Script>
		<![CDATA[
			import embeds.CSSSource;
			import embeds.HTMLSource;
		]]>
	</fx:Script>
	<fx:Declarations>
		<!-- Place non-visual elements (e.g., services, value objects) here -->
	</fx:Declarations>
	<views:WebView
		left="100"
		right="100"
		top="100"
		bottom="100"
		css="{new CSSSource.Default().toString()}"
		html="{new HTMLSource.Long().toString()}"/>
	<!--
	<views:WebView
		left="300"
		right="300"
		top="100"
		bottom="100"
		css="{new CSSSource.Default().toString()}"
		html="{new HTMLSource.Small().toString()}"/>
	-->
</s:Application>
